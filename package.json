{
  "name": "@priveted/ajax",
  "version": "1.0.7",
  "type": "module",
  "main": "./dist/ajax.umd.js",
  "module": "./dist/ajax.umd.mjs",
  "types": "./dist/ajax.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ajax.mjs",
      "require": "./dist/ajax.umd.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "description": "A modern and lightweight library for working with ajax, allowing you to use various request technologies in a single style.",
  "keywords": [
    "ajax",
    "requests",
    "xhr",
    "fetch"
  ],
  "homepage": "https://github.com/priveted/ajax",
  "repository": {
    "type": "git",
    "url": "https://github.com/priveted/ajax"
  },
  "license": "MIT",
  "author": "Eduard Y",
  "scripts": {
    "dev": "vite",
    "prepare": "npm run build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.0"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  }
}
